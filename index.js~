function Roll(n) {
return (1 + Math.floor(n * Math.random()));}





const Discord = require('discord.js'); 
const client = new Discord.Client();  
client.on('ready', () => {   
    console.log(`Logged in as ${client.user.tag}!`); 
});
client.on('message', msg => {  
    if (msg.content === 'Hello Die') {     
        msg.reply('Hello');}
    if (msg.content.includes('Roll', 0)) {
    	msg.reply(Roll(parseInt(msg.content.substring(5,7))));}
    if (msg.content.includes('roll', 0)) {
    	msg.reply(Roll(parseInt(msg.content.substring(5,7))));}
    if (msg.content === "up up down down left right left right b a") {
    msg.reply('Fuck you ')}
})
client.login('NjM3ODM3NTExNjgyNDI0ODMz.XbUB8g.fqnRq_qBQYXQi9uRNygr1nx5Jr0');